{
  "name": "html-frontmatter-mini",
  "version": "1.0.0",
  "description": "Extract key-value metadata from HTML comments",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "cs-fix": "standard --fix src/**/*.js test/**/*.js",
    "test": "mocha && standard src/**/*.js test/**/*.js",
    "prepublish": "./node_modules/.bin/babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spiasecki/html-frontmatter"
  },
  "keywords": [
    "html",
    "frontmatter",
    "comments",
    "metadata"
  ],
  "contributors": [
    {
      "name": "Zeke Sikelianos",
      "email": "zeke@sikelianos.com",
      "url": "http://zeke.sikelianos.com/"
    },
    {
      "name": "Szymon Piasecki",
      "url": "https://github.com/spiasecki/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spiasecki/html-frontmatter/issues"
  },
  "homepage": "https://github.com/spiasecki/html-frontmatter",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "mocha": "^5.2.0",
    "standard": "^12.0.1"
  },
  "dependencies": {},
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
